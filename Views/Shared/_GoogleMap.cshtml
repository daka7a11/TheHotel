@inject Microsoft.Extensions.Configuration.IConfiguration config

@{
    var googleApiKey = @config.GetSection("GoogleAPIKey").Value;
}

<script>
    var map //43.74689846916985, 27.018680680192933
    function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
            center: { lat: 43.74689846916985, lng: 27.018680680192933 },
            zoom: 20
        });
    }
</script>
<script async defer src="https://maps.googleapis.com/maps/api/js?key=@googleApiKey&callback=initMap"></script>